<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Suite" verbose="2">

	<groups>
		<run>
			<include name="regression"></include>
			<include name="common"></include>
		</run>


	</groups>

	<parameter name="URL" value="http://leaftaps.com/opentaps"></parameter>
	<parameter name="uName" value="DemoSalesManager"></parameter>
	<parameter name="pwd" value="crmsfa"></parameter>
	<parameter name="browser" value="chrome"></parameter>

	<test name="Test">
		<classes>
			<class name="testCases.CreateLeadWD" />
			<class name="testCases.DuplicateLeadWD" />
			<class name="testCases.MergeLeadWD" />
			<class name="testCases.EditLeadWD" />
			<class name="testCases.DeleteLeadWD" />
		</classes>
	</test> <!-- Test -->
</suite> <!-- Suite -->
